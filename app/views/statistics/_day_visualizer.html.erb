<div class="day">
  <div class='hours-graph'>
    <% day_polling_interval.each do |i| %>
      <% time = Time.at(i)%>
      <div 
        class="<%= history ? 'at-home ' : 'away '%><%= time.strftime('%M') == '00' ? ' hour' : '' %> <%= time.strftime('%H%M') %>" 
        title="<%= time.strftime('%H:%M') %>" 
        <%= "onclick=scrollRowToView('#{time.strftime('%H%M')}')" unless history %>>
      </div>
    <% end %>
  </div>
  <div class='hours'>
    <%('00'..'24').each do |i| %>
      <p><%= i %></p>
    <% end %>  
  </div>
</div>